#!/usr/bin/make -f
# See debhelper(7) (uncomment to enable)
# output every command that modifies files on the build system.
#export DH_VERBOSE = 1

# see ENVIRONMENT in dpkg-buildflags(1)
# package maintainers to append CFLAGS
export DEB_CFLAGS_MAINT_APPEND  = -O3 -fno-stack-protector
export DEB_CXXFLAGS_MAINT_APPEND  = -O3 -fno-stack-protector
export DEB_CPPFLAGS_MAINT_SET  = -D_FORTIFY_SOURCE=0


%:
	dh $@


